{
  "name": "json-structure",
  "version": "0.1.0",
  "description": "TypeScript/JavaScript SDK for validating JSON documents against JSON Structure schemas",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "json",
    "schema",
    "validation",
    "json-structure",
    "typescript"
  ],
  "author": "JSON Structure Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/json-structure/sdk.git",
    "directory": "typescript"
  },
  "bugs": {
    "url": "https://github.com/json-structure/sdk/issues"
  },
  "homepage": "https://json-structure.org",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
